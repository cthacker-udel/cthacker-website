{
    "name": "cthacker-website",
    "version": "0.1.0",
    "engines": {
        "node": ">=18.0.0"
    },
    "browserslist": [
        "defaults",
        "not ie <= 11",
        "not op_mini all"
    ],
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint-all": "eslint .",
        "clean": "rimraf .next",
        "full-clean": "rimraf .next node_modules",
        "format": "prettier --config .prettierrc --write src/**/*.{ts,tsx}",
        "install-linter": "npm i --save-dev eslint eslint-config-intense eslint-config-next eslint-config-prettier eslint-plugin-compat eslint-plugin-eslint-comments eslint-plugin-import eslint-plugin-jest eslint-plugin-jest-formatting eslint-plugin-jsx-a11y eslint-plugin-no-secrets eslint-plugin-node eslint-plugin-prettier eslint-plugin-promise eslint-plugin-ramda eslint-plugin-react eslint-plugin-react-hooks eslint-plugin-security eslint-plugin-simple-import-sort eslint-plugin-sonarjs eslint-plugin-tsdoc eslint-plugin-unicorn husky lint-staged prettier prettier-eslint typescript @typescript-eslint/eslint-plugin @typescript-eslint/parser @next/eslint-plugin-next",
        "export": "next export",
        "prepare": "husky install"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.4.2",
        "@fortawesome/free-solid-svg-icons": "^6.4.2",
        "@octokit/auth-token": "^4.0.0",
        "@octokit/request": "^8.1.1",
        "lodash.debounce": "^4.0.8",
        "next": "^13.4.19",
        "octokit": "^3.1.2",
        "react": "18.2.0",
        "react-bootstrap": "^2.8.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.46.1",
        "react-spinners": "^0.13.8",
        "react-toastify": "^9.1.3"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^13.4.19",
        "@octokit/types": "^11.1.0",
        "@types/lodash.debounce": "^4.0.7",
        "@types/node": "^20.5.9",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
        "babel-plugin-react-intl": "^8.2.25",
        "eslint": "^8.48.0",
        "eslint-config-intense": "^0.9.0",
        "eslint-config-next": "^13.4.19",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-problems": "^8.0.0",
        "eslint-plugin-compat": "^4.2.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jest": "^27.2.3",
        "eslint-plugin-jest-formatting": "^3.1.0",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-no-secrets": "^0.8.9",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-ramda": "^2.5.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-security": "^1.7.1",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-sonarjs": "^0.21.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "eslint-plugin-unicorn": "^48.0.1",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "prettier": "^3.0.3",
        "prettier-eslint": "^15.0.1",
        "ts-key-enum": "^2.0.12",
        "typescript": "^5.2.2"
